{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "light",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "orientation": "default",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moamen.fitFlow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.moamen.fitFlow"
    },
    "web": {
      "output": "server"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ]
    ],
    "name": "FitFlow",
    "slug": "FitFlow",
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "ec31948d-f989-4d7b-9954-427f24ed6897"
      }
    }
  }
}
